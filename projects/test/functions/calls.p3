
pub fn testCalls() {
    const a = {
        ret 0
    }
    const c = |int a, int b| {
        ret 5
    }

    //a()   // ambiguous
    //b(3)  // ambiguous
    
	assert 2 == b(3 as int) // b(int) exact match
    assert 3 == b(3.1)      // b(float)
    assert 3 == b(p1:3.1)
    assert 4 == b(1,2)
    assert 4 == b(p1:1, p2:3.1) 
    assert 4 == b(p2:3.1, p1:1)
    assert 5 == c(7,8)
    assert 5 == c(a:7, b:8)
    assert 5 == c(b:7, a:9)
	
    const testGroovyStyleCall = {
        println("Test ... Groovy type function calls")
        // declarations
		const blah = |fn()void closure| {
            closure()
        }
        const blah2 = |int a, fn()void closure| {
            closure()
        }

		//calls
        blah()   { println("hello") }
        blah2(1) { println("hello2") }
    }
    testGroovyStyleCall()
}

fn a() {
    ret 1
}
fn b(int p1) {
    ret 2
}
fn b(float p1) {
    ret 3
}
fn b(int p1, float p2) {
    ret 4
}
